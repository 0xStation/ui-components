{
  "name": "@station/ui-components",
  "version": "0.0.1",
  "description": "Station UI components",
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration",
    "check": "tsc --pretty && pnpm prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "restricted",
    "linkDirectory": false
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-vite": "^7.0.24",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.2.14",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.24",
    "tailwindcss": "^3.3.2",
    "tsup": "^7.1.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": "*",
    "react-dom": "*"
  },
  "main": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xStation/ui-components.git"
  },
  "keywords": [
    "components"
  ],
  "author": "station",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/0xStation/ui-components/issues"
  },
  "homepage": "https://github.com/0xStation/ui-components#readme"
}
